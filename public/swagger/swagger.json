{
    "swagger": "2.0",
    "info": {
        "title": "Enten swagger docs",
        "description": "Getting started with Laravel and Swagger",
        "termsOfService": "",
        "contact": {
            "email": "okay7290@gmail.com"
        },
        "version": "1.0.0"
    },
    "host": "localhost:8000",
    "basePath": "/",
    "schemes": [
        "http"
    ],
    "paths": {
        "/api/v1/customer/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Get a JWT via given credentials",
                "description": "Returns a JWT token",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "email",
                        "in": "formData",
                        "description": "Email address",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "description": "Password",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "properties": {
                                "error": {
                                    "type": "boolean",
                                    "example": false
                                },
                                "data": {
                                    "properties": {
                                        "access_token": {
                                            "type": "string",
                                            "example": "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJzdWIiOjEsImlzcyI6Imh0dHA6Ly9sb2NhbGhvc3QvYXBpL2xvZ2luIiwiaWF0IjoxNjIwNjIxNjA4LCJleHAiOjE2MjA2Mjc2MDgsIm5iZiI6MTYyMDYyMTYwOCwianRpIjoiTzRVMTM4Q2Fpd1JjM2lxbSJ9.SD_iJmpMSjcnhldl-SP_Gb0Lv9pTde0J32A-JfKzZoU"
                                        },
                                        "token_type": {
                                            "type": "string",
                                            "example": "bearer"
                                        },
                                        "expires_in": {
                                            "type": "integer",
                                            "example": 3600
                                        }
                                    },
                                    "type": "object"
                                }
                            },
                            "type": "object"
                        },
                        "headers": {
                            "Authorization": {
                                "description": "Bearer {token}",
                                "type": "string"
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request data",
                        "schema": {
                            "properties": {
                                "error": {
                                    "type": "boolean",
                                    "example": true
                                },
                                "message": {
                                    "type": "object",
                                    "example": {
                                        "email": [
                                            "The email field is required."
                                        ]
                                    }
                                },
                                "type": {
                                    "type": "integer",
                                    "example": 400
                                }
                            },
                            "type": "object"
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/v1/customer/register": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Register a new customer",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "name",
                        "in": "formData",
                        "description": "Name of the customer",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "email",
                        "in": "formData",
                        "description": "Email of the customer",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "description": "Password of the customer",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password_confirmation",
                        "in": "formData",
                        "description": "Confirm password of the customer",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Customer registered successfully",
                        "schema": {
                            "properties": {
                                "customer": {
                                    "$ref": "#/definitions/Customer"
                                },
                                "token": {
                                    "description": "The JWT token for the authenticated user",
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "schema": {
                            "properties": {
                                "error": {
                                    "type": "boolean",
                                    "example": true
                                },
                                "message": {
                                    "description": "Validation error messages",
                                    "type": "object"
                                },
                                "type": {
                                    "description": "Error type code",
                                    "type": "integer"
                                }
                            },
                            "type": "object"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "Customer": {
            "required": [
                "name",
                "email"
            ],
            "properties": {
                "name": {
                    "description": "Name of the customer",
                    "type": "string"
                },
                "email": {
                    "description": "Email of the customer",
                    "type": "string"
                },
                "phone": {
                    "description": "Phone number of the customer",
                    "type": "string"
                }
            },
            "type": "object"
        }
    }
}